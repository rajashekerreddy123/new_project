%h1 Listing attendences
%table
  %tr
    %th Group
    %th Year
  - @attendences.each do |attendence|
    %tr
      %td= h attendence.group
      %td= h attendence.year
      %td= link_to 'Show', attendence
      %td= link_to 'Edit', edit_attendence_path(attendence)
      %td= link_to 'Destroy', attendence, :confirm => 'Are you sure?', :method => :delete
%br/
= link_to 'New attendence', new_attendence_path
